cat lopunny0/starly swoobat3/growlithe > rattata8_29
cp ferrothorn5 porygon1/geodudeferrothorn
ln -s ferrothorn5 lopunny0/starlyferrothorn



cd ..
chmod u=rwx lab0/swoobat3
wc -m $(grep -Rl . lab0 | grep 'e$') | sort -r
ls -lRtr lab0 2>/dev/null | grep -E '^-.* [^ ]*s[^ ]*$' | head -2
cat -n lab0/rattata8 | sort -k2
cat -n $(grep -Rl . lab0 2>/tmp/errors| grep 't$' || echo "nonexistent") 2>>/tmp/errors | sort -r -k2
cat lab0/rattata8 2>/dev/null | sort -r
wc -m lab0/lopunny0/darmanitan lab0/lopunny0/starly lab0/lopunny0/golduck lab0/porygon1/geodude lab0/swoobat3/dewgong lab0/swoobat3/pelipper lab0/swoobat3/growlithe 2>&1 | sort


chmod u=rwx lab0/taillow2
rm lab0/taillow2
chmod -R 777 lab0/swoobat3
